function main;
beginparams
endparams

beginlocals

  n : integer;
  i, j, k: integer;
  t : array [20] of integer;

endlocals
beginbody
  k := 0;
  n := 20;
  
  do 
    beginloop
       t[k] := k;
       k := k + 1;
       if k % 2 == 0 then
           continue;
       endif;
       write t[k-1];
    endloop
  while k < n;
endbody
